{% extends "base-test.stencil" %}

{% block file_contents %}
{% set test_contents %}{% block test_contents %}{% endblock %}{% endset %}
{% for Matrix in types.implementing.MatrixProtocol|struct %}
{% for Scalar in types.based.Numeric|!protocol %}
final class {{ base_name_var }}{{ Matrix.name }}{{ Scalar.name }}: XCTestCase {
    typealias Element = {{ Scalar.name }}
    typealias TestMatrix = {{ Matrix.name }}<{{ Scalar.name }}>
    {{ test_contents }}
}

{% endfor %}
{% endfor %}
{% endblock %}
